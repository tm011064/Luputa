    <objects minutes="10" enabled="true">
      <add name="UniqueUser"            enabled="true"      cacheItemPriority="Normal"      isIterating="true" suffix="_UU" />
    </objects>
  </Cache>-->
	<!-- Job Example: 
  -->
	<Jobs>
		<jobPool minutes="10">
			<add name="FirstJob" type="CommonTools.TestApp.Jobs.FirstJob, App_Code" enabled="true" enableShutDown="false" firstRunAtInitialization="true">
				<Options>
					<add name="FirstValue" value="val1"/>
					<add name="SecondValue" value="val2"/>
					<add name="ThirdValue" value="val3"/>
				</Options>
			</add>
			<add name="SecondJob" type="CommonTools.TestApp.Jobs.SecondJob, App_Code" enabled="true" enableShutDown="false" firstRunAtInitialization="false"/>
			<add name="NoSingleJob" type="CommonTools.TestApp.Jobs.NoSingleJob, App_Code" enabled="true" enableShutDown="false" executeOnOwnThread="true" seconds="10"/>
			<add name="DailyJob" type="CommonTools.TestApp.Jobs.DailyJob, App_Code" enabled="true" enableShutDown="false" executeDaily="true" dailyUTCExecutionTime="11:11:00.000"/>
		</jobPool>
	</Jobs>
	<appSettings>
		<add key="SitemapControllerPath" value="C:\VSS\CommonTools.3.5.root\CommonTools.3.5\CommonTools.TestSuite\Resources\SitemapController.xml"/>
